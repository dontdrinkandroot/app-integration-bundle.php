{
    "name": "dontdrinkandroot/app-integration-bundle",
    "description": "Bundle that helps you building an api-platform backed Angular PWA",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\AppIntegrationBundle\\": ""
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/config": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/process": "^4.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "api-platform/core": "^2.2",
        "symfony/security-core": "^4.0",
        "phpunit/phpunit": "^6.0"
    },
    "suggest": {
        "api-platform/core": "For Api Platform Integration"
    }
}
